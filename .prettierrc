{
  "arrowParens": "avoid",
  "importOrder": ["<BUILTIN_MODULES>", "", "<THIRD_PARTY_MODULES>", "", "^src/", "", "^\\.\\.(/|$)", "", "^\\.(/|$)"],
  "importOrderParserPlugins": ["typescript", "decorators-legacy"],
  "importOrderTypeScriptVersion": "5.8.3",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 80,
        "proseWrap": "always"
      }
    }
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "printWidth": 120,
  "proseWrap": "preserve",
  "singleQuote": true,
  "trailingComma": "none"
}
